{
	"extends": "@tsconfig/node22/tsconfig.json",
	"include": [
		"vite.config.*",
		"vitest.config.*",
		"cypress.config.*",
		"playwright.config.*",
		"vite.config.ts",
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.tsx",
		"src/**/*.vue"
	],
	"compilerOptions": {
		"composite": true,
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"types": ["node"]
	}
}
